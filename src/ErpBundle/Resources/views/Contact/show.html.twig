{% extends '::base.html.twig' %}
{% block body %}

    <h1>Contact</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Client</th>
                <td>{{ entity.client }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ entity.user }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Desctiption</th>
                <td>{{ entity.desctiption }}</td>
            </tr>
            <tr>
                <th>Nextdatecontact</th>
                <td>{{ entity.nextDateContact|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Userdoer</th>
                <td>{{ entity.userDoer }}</td>
            </tr>
            <tr>
                <th>Project</th>
                <td>{{ entity.project }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('contact') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contact_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
