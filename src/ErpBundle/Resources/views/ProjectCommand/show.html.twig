{% extends '@Erp/layout.html.twig' %}
{% block body %}
    {{ parent() }}
    <h1>ProjectCommand</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ entity.user }}</td>
            </tr>
            <tr>
                <th>Project</th>
                <td>{{ entity.project }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('projectcommand') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('projectcommand_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
